{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "../build",
    "rootDir": "./",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "inlineSourceMap": true
  }
}
