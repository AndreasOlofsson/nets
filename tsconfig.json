{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "DOM",
      "ES2015",
      "ES2015.core",
      "ES2015.Promise"
    ],
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "baseUrl": "./src",
    "inlineSourceMap": true,
    "esModuleInterop": true
  }
}
